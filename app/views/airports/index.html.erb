<p id="notice"><%= notice %></p>

<h1>Airports</h1>

<table>
  <thead>
    <tr>
      <th>Oa</th>
      <th>Ident</th>
      <th>Type</th>
      <th>Name</th>
      <th>Lat</th>
      <th>Long</th>
      <th>Elevation ft</th>
      <th>Continent</th>
      <th>Iso country</th>
      <th>Iso region</th>
      <th>Municipality</th>
      <th>Scheduled service</th>
      <th>Gps code</th>
      <th>Iata code</th>
      <th>Home link</th>
      <th>Wikipedia link</th>
      <th>Keywords</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @airports.each do |airport| %>
      <tr>
        <td><%= airport.oa_id %></td>
        <td><%= airport.ident %></td>
        <td><%= airport.airport_type %></td>
        <td><%= airport.name %></td>
        <td><%= airport.lat %></td>
        <td><%= airport.long %></td>
        <td><%= airport.elevation_ft %></td>
        <td><%= airport.continent %></td>
        <td><%= airport.iso_country %></td>
        <td><%= airport.iso_region %></td>
        <td><%= airport.municipality %></td>
        <td><%= airport.scheduled_service %></td>
        <td><%= airport.gps_code %></td>
        <td><%= airport.iata_code %></td>
        <td><%= airport.home_link %></td>
        <td><%= airport.wikipedia_link %></td>
        <td><%= airport.keywords %></td>
        <td><%= link_to 'Show', airport %></td>
        <td><%= link_to 'Edit', edit_airport_path(airport) %></td>
        <td><%= link_to 'Destroy', airport, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Airport', new_airport_path %>
